{
	"name": "trello_backend",
	"version": "1.0.0",
	"description": "Backend for Trello Multiboard app",
	"main": "server/server.js",
	"scripts": {
		"client-install": "cd client && npm install",
		"client-build": "cd client && npm run build && cd ..",
		"start": "node server/server.js",
		"server": "nodemon -r esm server/server.js",
		"client": "npm start --prefix client",
		"dev": "concurrently \"npm run ssr\" \"npm run client\"",
		"ssr": "nodemon -r esm server/index.js",
		"all": "npm run client-build && npm run dev"
	},
