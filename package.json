{
	"name": "trello_backend",
	"version": "1.0.0",
	"description": "Backend for Trello Multiboard app",
	"main": "server/server.js",
	"scripts": {
		"client-install": "cd client && npm install",
		"client-build": "cd client && npm run build && cd ..",
		"start": "node server/server.js",
		"server": "nodemon -r esm server/server.js",
		"client": "npm start --prefix client",
		"dev": "concurrently \"npm run ssr\" \"npm run client\"",
		"ssr": "nodemon -r esm server/index.js",
		"all": "npm run client-build && npm run dev"
	},
	"author": "Jakub Stefanisin, xstefa22@stud.fit.vutbr.cz",
	"license": "ISC",
	"dependencies": {
		"@babel/plugin-proposal-class-properties": "^7.8.3",
		"@babel/preset-react": "^7.9.1",
		"@babel/register": "^7.9.0",
		"@babel/runtime": "^7.9.2",
		"babel-core": "^6.26.3",
		"babel-polyfill": "^6.26.0",
		"babel-preset-env": "^1.7.0",
		"babel-preset-react-app": "^9.1.2",
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.19.0",
		"concurrently": "^5.1.0",
		"cookie-parser": "^1.4.5",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"esm": "^3.2.25",
		"express": "^4.17.1",
		"express-session": "^1.17.0",
		"fs": "0.0.1-security",
		"http": "0.0.0",
		"ignore-styles": "^5.0.1",
		"jsonwebtoken": "^8.5.1",
		"logger": "0.0.1",
		"mongoose": "^5.9.5",
		"morgan": "^1.10.0",
		"nodemon": "^2.0.1",
		"npm-run-all": "^4.1.5",
		"oauth": "^0.9.15",
		"passport": "^0.4.1",
		"passport-trello": "^0.1.5",
		"react-dom": "^16.13.1",
		"react-dom-server": "0.0.5",
		"react-redux": "^7.2.0",
		"react-router-dom": "^5.1.2",
		"react-session-api": "^1.1.0",
		"react-universal": "0.0.5",
		"redux": "^4.0.4",
		"socket.io": "^2.3.0",
		"url": "^0.11.0",
		"webpack": "4.42",
		"webpack-bundle-analyzer": "^3.6.1",
		"webpack-cli": "^3.3.11",
		"webpack-node-externals": "^1.7.2"
	}
}
